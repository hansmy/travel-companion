<!--start: Container -->
	<div class="container">
	
		<!--start: Wrapper-->
		<div id="wrapper">
			
			<div id="filters">
				<ul class="option-set pull-left" data-option-key="filter">
				
					<li><a {{action 'recent'}} {{bind-attr class="sortAscending::selected"}}  >Recent</a></li>

					<li>/</li>
					<li><a {{action 'oldest'}} {{bind-attr class="sortAscending:selected"}} >Oldest</a></li>
				
					 
				</ul>
			</div>

			{{#if hasNewTweets}}

			<span >You have {{queueLength}} new tweets. <button {{action "unqueueAll"}} class="btn btn-default">Load</button> </span>
    		{{/if}}
										

			<div id="wall">
				{{#collection App.EListView contentBinding="controller.arrangedContent"}}
        	
        	 			{{view App.TweetView contentBinding="controller.arrangedContent"}}
        	
        	 	{{else}}
				   	 loading latest Tweets ...
			
      			{{/collection}}
				
 			 </div>
			<!--</div>-->
		</div>
	</div>
</div>
